
<div class="row">
<%= form_for(@project, {url: user_project_path(current_user, @project), project_id: @project.id, class: "inline-list",  remote: true}) do |f| %>
  <li><%= f.text_field :title, :value => @project.title, id: "projectInput#{@project.id}" %></li>
  <li><%= f.submit "Update", id: "projectUpdate#{@project.id}"%></li>
<% end %>
</div>
