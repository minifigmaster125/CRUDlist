
if (<%=@model.CRUDcreate%> == 0){
  $(".model__select__create__<%= @model.id %>").attr("src","<%= asset_path('NotStarted.png') %>" );
} else if (<%=@model.CRUDcreate%> == 1){
  $(".model__select__create__<%= @model.id %>").attr("src", "<%= asset_path('Done.png') %>");
} else if (<%=@model.CRUDcreate%> == 2){
  $(".model__select__create__<%= @model.id %>").attr("src","<%= asset_path('InProgress.png') %>" );
} else if (<%=@model.CRUDcreate%> == 3){
  $(".model__select__create__<%= @model.id %>").attr("src","<%= asset_path('NotApplicable.png') %>" );
}

